	<section class="container">
		<div class="contaier-fuild">
			<div class="row">
				<div id="slide" class="carousel slide slideShow" data-ride="carousel">
					
					<ol class="carousel-indicators">
						<li data-target="#slide" data-slide-to="0" class="active"></li>
						<li data-target="#slide" data-slide-to="1"></li>
						<li data-target="#slide" data-slide-to="2"></li>
						<li data-target="#slide" data-slide-to="3"></li>
					</ol>	

					<div class="carousel-inner" role="listbox">
						<div class="item active">
							<img src="/assets/images/img1.jpg" alt="img1">
						</div>
						<div class="item">
							<img src="/assets/images/img2.jpg" alt="img1">
						</div>
						<div class="item">
							<img src="/assets/images/img3.jpg" alt="img1">
						</div>
						<div class="item">
							<img src="/assets/images/img4.jpg" alt="img1">
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>

	<div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="/logout/sign-out">
        Logout
      </a>
    </div>
  </div>
  	<section class="container">
		<div class="row">
		{{#each products}}
            <div class="col-xs-6 col-sm-3 col-md-3 col-lg-3">
            <div class="thumbnail">
				<a href="#" data-toggle="modal" data-target="#{{this.id}}">
				<img src="{{this.img}}" alt="...">
				</a>
				<div class="thumbnail-product">
				<h5 href="#">{{this.itemNum}}</h5>
				<a href="#">{{this.itemName}}</a>
            	</div>
            </div>
            </div>
        {{/each}} 
        </div>
    </section> 

      



    
      <!-- Modal content-->

	{{#each products}}
	<div class="modal fade" id="{{this.id}}" role="dialog">
    	<div class="modal-dialog">
    
      		<div class="modal-content">
        		<div class="modal-header">
          			<button type="button" class="close" data-dismiss="modal">&times;</button>
          			<h4 class="modal-title">{{this.itemName}}</h4>
        		</div>
        		<div class="container-fluid">
        			<div class="row">
        				<div class="col-xs-12 col-md-6">
        					<img src="{{this.img}}" alt="..." class="modalImg">
        				</div>

        			</div>
        		</div>
     		</div>
      
   		</div>
	</div>
     {{/each}}    




